{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "thanhfactory"
		},
		"FactoryAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'FactoryAzureDataLakeStorage'"
		},
		"FactorySql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FactorySql'"
		},
		"FactoryAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://thanhstorage.dfs.core.windows.net/"
		},
		"FactorySql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "thanhsqldbserver.database.windows.net"
		},
		"FactorySql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "thanhdb"
		},
		"FactorySql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "thanhadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "thanhfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FactoryAzureDataLakeStorage_accountKey": {
						"value": "[parameters('FactoryAzureDataLakeStorage_accountKey')]"
					},
					"FactorySql_password": {
						"value": "[parameters('FactorySql_password')]"
					},
					"FactoryAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('FactoryAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"FactorySql_properties_typeProperties_server": {
						"value": "[parameters('FactorySql_properties_typeProperties_server')]"
					},
					"FactorySql_properties_typeProperties_database": {
						"value": "[parameters('FactorySql_properties_typeProperties_database')]"
					},
					"FactorySql_properties_typeProperties_userName": {
						"value": "[parameters('FactorySql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}